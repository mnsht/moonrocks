{
  "name": "sr-components",
  "version": "1.0.0",
  "description": "The UI component library for Scholar Raise, using Storybook",
  "source": "index.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "start": "start-storybook",
    "build": "rm -rf ./dist && microbundle --name sr-components --jsx React.createElement --globals styled-system=styled-system,styled-components=styled-components"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scholarraise/sr-components.git"
  },
  "author": "Patrick Cason <patrick@scholarraise.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scholarraise/sr-components/issues"
  },
  "homepage": "https://github.com/scholarraise/sr-components#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-storysource": "^5.0.6",
    "@storybook/addon-viewport": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-loader": "^8.0.5",
    "husky": "^1.3.1",
    "microbundle": "^0.11.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0",
    "styled-system": "^4.1.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0",
    "styled-system": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn build"
    }
  },
  "dependencies": {
    "cleave.js": "^1.4.10"
  }
}
