{
  "name": "moonrocks",
  "version": "1.1.0",
  "description": "Just another UI component library based on Styled System.",
  "source": "index.js",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "yarn build && source-map-explorer dist/index.js dist/index.js.map",
    "start": "start-storybook",
    "build": "rm -rf ./dist && rollup -c",
    "prepare": "yarn build",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "release": "np",
    "test": "echo \"Error: no test specified\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnsht/moonrocks.git"
  },
  "author": "Moonshot <contact@mns.ht>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mnsht/moonrocks/issues"
  },
  "homepage": "https://github.com/mnsht/moonrocks#readme",
  "keywords": [
    "styled-system",
    "styled system",
    "rebass",
    "ui",
    "component",
    "storybook",
    "library",
    "moonshot",
    "mnsht"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn build"
    }
  },
  "dependencies": {
    "cleave.js": "1.4.10",
    "formik": "1.5.7",
    "react-pose": "^4.0.8",
    "react-select": "^2.4.3",
    "styled-components": "^4.2.0",
    "styled-system": "^4.1.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@sambego/storybook-state": "^1.3.4",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-storysource": "^5.0.11",
    "@storybook/addon-viewport": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "auto-changelog": "^1.13.0",
    "babel-loader": "^8.0.5",
    "husky": "^2.1.0",
    "np": "^5.0.3",
    "prettier": "^1.17.0",
    "pretty-quick": "1.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-router-dom": "^5.0.0",
    "rollup": "1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.2",
    "rollup-plugin-url": "^2.2.2",
    "source-map-explorer": "^1.8.0",
    "storybook-react-router": "^1.0.5"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-router-dom": "^5.0.0"
  }
}
